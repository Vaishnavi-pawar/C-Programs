#include<stdio.h>
void PrintEven(int iNo)
{
    int iCnt = 1;
    if(iNo <=0)
    {
    return;
    }
   while(iNo>=iCnt)
    {
        printf("%d\n",2*iCnt);
        iCnt++;
    }
}
   int main()
   {
    int iValue = 0;
    printf("Enter number\n");
    scanf("%d",&iValue);
    PrintEven(iValue);
    return 0;
   }
